{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "src/*": [
        "./*",
      ],
    },
    "outDir": "dist",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
  ]
}
