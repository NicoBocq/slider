{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": [
      "./tsconfig.json"
    ],
    "createDefaultProgram": true
  },
  "env": {
    "browser": true
  },
  "globals": {
    "process": "readonly"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "ignorePatterns": "**/*.js",
  "rules": {
    "no-console": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off"
  }
}
